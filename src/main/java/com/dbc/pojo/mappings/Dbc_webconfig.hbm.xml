<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbc.pojo.Dbc_webconfig" table="dbc_webconfig">
        <id name="id" column="ID" type="java.lang.String">
            <generator class="uuid.hex"></generator>
        </id>
        <property name="isopen" column="ISOPEN" type="string" lazy="true"/>
        <property name="closemsg" column="CLOSEMSG" type="string" lazy="true"/>
        <property name="logo" column="LOGO" type="string" lazy="true"/>
        <property name="webname" column="WEBNAME" type="string" lazy="true"/>
        <property name="webnickname" column="WEBNICKNAME" type="string" lazy="true"/>
        <property name="webtitle" column="WEBTITLE" type="string" lazy="true"/>
        <property name="webkey" column="WEBKEY" type="string" lazy="true"/>
        <property name="webdescription" column="WEBDESCRIPTION" type="string" lazy="true"/>
        <property name="weburl" column="WEBURL" type="string" lazy="true"/>
        <property name="webfoot" column="WEBFOOT" type="string" lazy="true"/>
        <property name="topic" column="TOPIC" type="string" lazy="true"/>
        <property name="is_static_community" column="IS_STATIC_COMMUNITY" type="string" lazy="true"/>
        <property name="is_static_street" column="IS_STATIC_STREET" type="string" lazy="true"/>
        <property name="description" column="DESCRIPTION" type="string" lazy="true"/>
        <property name="sortcode" column="SORTCODE" type="double" lazy="true"/>
        <property name="createdate" column="CREATEDATE" type="string" lazy="true"/>
        <property name="createuser" column="CREATEUSER" type="string" lazy="true"/>
        <property name="updatedate" column="UPDATEDATE" type="string" lazy="true"/>
        <property name="updateuser" column="UPDATEUSER" type="string" lazy="true"/>
        <property name="deletemark" column="DELETEMARK" type="string" lazy="true" not-null="true"/>
    </class>
</hibernate-mapping>