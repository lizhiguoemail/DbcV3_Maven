<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbc.pojo.Dbc_article" table="dbc_article">
        <id name="id" column="ID" type="java.lang.String">
            <generator class="uuid.hex"></generator>
        </id>
        <property name="title" column="TITLE" type="string" lazy="true"/>
        <property name="treeid" column="TREEID" type="string" lazy="true"/>
        <property name="content" column="CONTENT" type="text" lazy="true"/>
        <property name="source" column="SOURCE" type="string" lazy="true"/>
        <property name="keywords" column="KEYWORDS" type="string" lazy="true"/>
        <property name="filepath" column="FILEPATH" type="string" lazy="true"/>
        <property name="istop" column="ISTOP" type="string" lazy="true"/>
        <property name="isindex" column="ISINDEX" type="string" lazy="true"/>
        <property name="readnum" column="READNUM" type="long" lazy="true"/>
        <property name="author" column="AUTHOR" type="string" lazy="true"/>
        <property name="ishasttachment" column="ISHASTTACHMENT" type="string" lazy="true"/>
        <property name="ishot" column="ISHOT" type="string" lazy="true"/>
        <property name="ishidden" column="ISHIDDEN" type="string" lazy="true"/>
        <property name="isrecommend" column="ISRECOMMEND" type="string" lazy="true"/>
        <property name="isnew" column="ISNEW" type="string" lazy="true"/>
        <property name="article_flag" column="ARTICLE_FLAG" type="string" lazy="true"/>
        <property name="title_pic" column="TITLE_PIC" type="string" lazy="true"/>
        <property name="summary" column="SUMMARY" type="string" lazy="true"/>
        <property name="isshowsummary" column="ISSHOWSUMMARY" type="string" lazy="true"/>
        <property name="description" column="DESCRIPTION" type="string" lazy="true"/>
        <property name="sortcode" column="SORTCODE" type="double" lazy="true"/>
        <property name="createdate" column="CREATEDATE" type="string" lazy="true"/>
        <property name="createuser" column="CREATEUSER" type="string" lazy="true"/>
        <property name="updatedate" column="UPDATEDATE" type="string" lazy="true"/>
        <property name="updateuser" column="UPDATEUSER" type="string" lazy="true"/>
        <property name="deletemark" column="DELETEMARK" type="string" lazy="true" not-null="true"/>
    </class>
</hibernate-mapping>