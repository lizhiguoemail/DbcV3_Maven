<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbc.pojo.Dbc_app" table="dbc_app">
        <id name="id" column="ID" type="java.lang.String">
            <generator class="uuid.hex"></generator>
        </id>
        <property name="name" column="NAME" type="string" lazy="true"/>
        <property name="apptype" column="APPTYPE" type="string" lazy="true"/>
        <property name="tuijiandengji" column="TUIJIANDENGJI" type="string" lazy="true"/>
        <property name="shouquanfangshi" column="SHOUQUANFANGSHI" type="string" lazy="true"/>
        <property name="liulancishu" column="LIULANCISHU" type="string" lazy="true"/>
        <property name="fenbianlv" column="FENBIANLV" type="string" lazy="true"/>
        <property name="jianjie" column="JIANJIE" type="text" lazy="true"/>
        <property name="smallpic" column="SMALLPIC" type="string" lazy="true"/>
        <property name="ylurl" column="YLURL" type="string" lazy="true"/>
        <property name="zuozhe" column="ZUOZHE" type="string" lazy="true"/>
        <property name="slt1" column="SLT1" type="string" lazy="true"/>
        <property name="slt2" column="SLT2" type="string" lazy="true"/>
        <property name="slt3" column="SLT3" type="string" lazy="true"/>
        <property name="slt4" column="SLT4" type="string" lazy="true"/>
        <property name="slt5" column="SLT5" type="string" lazy="true"/>
        <property name="description" column="DESCRIPTION" type="string" lazy="true"/>
        <property name="sortcode" column="SORTCODE" type="double" lazy="true"/>
        <property name="createdate" column="CREATEDATE" type="string" lazy="true"/>
        <property name="createuser" column="CREATEUSER" type="string" lazy="true"/>
        <property name="updatedate" column="UPDATEDATE" type="string" lazy="true"/>
        <property name="updateuser" column="UPDATEUSER" type="string" lazy="true"/>
        <property name="deletemark" column="DELETEMARK" type="string" lazy="true" not-null="true"/>
    </class>
</hibernate-mapping>