<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbc.pojo.Dbc_nav" table="dbc_nav">
        <id name="id" column="ID" type="java.lang.String">
            <generator class="uuid.hex"></generator>
        </id>
        <property name="title" column="TITLE" type="string" lazy="true"/>
        <property name="url" column="URL" type="string" lazy="true"/>
        <property name="tip" column="TIP" type="string" lazy="true"/>
        <property name="hide" column="HIDE" type="string" lazy="true"/>
        <property name="type" column="TYPE" type="string" lazy="true"/>
        <property name="auto" column="AUTO" type="string" lazy="true"/>
        <property name="target" column="TARGET" type="string" lazy="true"/>
        <property name="state" column="STATE" type="string" lazy="true"/>
        <property name="custom" column="CUSTOM" type="string" lazy="true"/>
        <property name="nav_act" column="NAV_ACT" type="string" lazy="true"/>
        <property name="nav_mod" column="NAV_MOD" type="string" lazy="true"/>
        <property name="tag" column="TAG" type="string" lazy="true"/>
        <property name="pid" column="PID" type="string" lazy="true"/>
        <property name="alt" column="ALT" type="string" lazy="true"/>
        <property name="sys" column="SYS" type="string" lazy="true"/>
        <property name="plugin" column="PLUGIN" type="string" lazy="true"/>
        <property name="description" column="DESCRIPTION" type="string" lazy="true"/>
        <property name="sortcode" column="SORTCODE" type="double" lazy="true"/>
        <property name="createdate" column="CREATEDATE" type="string" lazy="true"/>
        <property name="createuser" column="CREATEUSER" type="string" lazy="true"/>
        <property name="updatedate" column="UPDATEDATE" type="string" lazy="true"/>
        <property name="updateuser" column="UPDATEUSER" type="string" lazy="true"/>
        <property name="deletemark" column="DELETEMARK" type="string" lazy="true" not-null="true"/>
    </class>
</hibernate-mapping>